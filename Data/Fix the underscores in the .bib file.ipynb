   "source": [
    "# Read in the file\n",
    "filedata = None\n",
    "with open('unsupervisedreview.bib', 'r') as file :\n",
    "  filedata = file.read()\n",
    "\n",
    "# Replace the target string\n",
    "filedata = filedata.replace('_', '\\_')\n",
    "\n",
    "# Write the file out again\n",
    "with open('unsupervisedreview.bib', 'w') as file:\n",
    "  file.write(filedata)"
   ]
